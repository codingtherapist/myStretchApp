			</section>
			<!--Stretchbox-->
			<section class="spotlight style1 content-align-left image-position-center onscroll-image-fade-in stretchcontainer" >					
				<div class="firstcontainer" >
					<h2 class=" center">1. Start timer</h2>
					<div class="center" id="app">
					</div>
					<div class="center" id="">
						<input type="button" onclick="starTimer()" value="Start" />
						<input type="button" " onclick="pauseTimer()" value="Pause"/>
						<input type="button" onclick="resetTimer()" value="Reset" /><br><br>
					</div><br><br>
					<!-- Stretchbox -->
					<div class="bodypartsbox" id="stretchbuttons">
						<h2 class="center">2. Choose a body part to stretch</h2>
						<div id="timerbuttons" >
							<input type="button" " id="eyesnext" value="eyes"/>
							<input type="button" " id="necknext" value="Neck"/>
							<input type="button" " id="shouldersnext" value="Shoulders"/>
							<input type="button" " id="handnext" value="Hands"/>
						</div>							
						<br><p class="center"> Hold or gently move through each pose for 3-5 breaths</p>
					</div>
					<img id="eyesup" class="hidden" src="https://res.cloudinary.com/dtamwfybo/image/upload/v1669501486/Untitled_design_-_2022-11-26T172925.648_jds1qr.png" alt="woman seated with hands together at chest" />
